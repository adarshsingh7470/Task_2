pipeline {
  agent any
  stages {
    stage('Setup') {
      steps {
        script {
          // Install Flask module
          sh 'pip3 install flask'
        }
      }
    }
    stage('Run Application') {
      steps {
        sh 'python3 app.py'
      }
    }
    stage('Run Website Tests') {
      steps {
        sh 'python3 website_test-.py'
      }
    }
    // Add more stages as needed
  }
}
